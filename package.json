{
  "name": "nuxt-mocha-composition-api-unit-tests",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "test": "mochapack --webpack-config node_modules/nuxt/webpack.config.js --require test/helpers/setup.ts test/spec/**/*.{ts,js}", 
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/composition-api": "^0.22.4",
    "@nuxtjs/pwa": "^3.3.5",
    "core-js": "^3.12.1",
    "nuxt": "^2.15.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-typescript": "^7.13.0",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@nuxt/types": "^2.15.5",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/tailwindcss": "^4.1.3",
    "@types/mocha": "^8.2.2",
    "@types/power-assert": "^1.5.3",
    "@typescript-eslint/parser": "^4.26.1",
    "@vinayakkulkarni/prettier-config-vue": "^1.0.0",
    "@vue/test-utils": "^1.2.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^8.0.3",
    "husky": "^4.3.8",
    "jsdom": "^16.6.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^11.0.0",
    "mocha": "^8.3.2",
    "mochapack": "^2.0.6",
    "postcss": "^8.3.2",
    "power-assert": "^1.6.1",
    "prettier": "^2.2.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "typescript": "^4.3.2"
  }
}
